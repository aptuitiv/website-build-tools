<body>
    {{ _page.bodyStartCode() }}
    <div class="visually-hidden" aria-hidden="true">{{ source('snippets/svg-icons.twig') }}</div>
    <header class="Header" id="top">
        <div class="Container">
            <div class="grid nc-2 align-center justify-between">
                <div class="col-3-4 col-1-3-xl">
                    {% if _core.theme.settings.headerLogo %}
                        <a href="/" title="return to home page" class="Header-logoLink">
                            {% set _core.theme.settings.headerLogo.class = "Header-logo" %}
                            {% do _core.theme.settings.headerLogo.resize(_core.theme.settings.headerLogoWidth + 100) %}
                            {{ _core.theme.settings.headerLogo.tag }}
                        </a>
                    {% endif %}
                </div>
                <div class="col-1-4 flex justify-end hidden-xl">
                    <button class="SmScBtn js-ssNavBtn" aria-label="Navigation menu" aria-expanded="false" aria-controls="navbar"><span class="SmScBtn-icon"></span>Menu</button>
                </div>
                <div class="col-1-1 col-2-3-xl flex-xl justify-end-xl Header-nav">
                    {{ _api.navigation.get.key('main') }}
                </div>
            </div>
        </div>
    </header>
